<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
   <xs:element name="passportdata">
             <xs:complexType>
			    <xs:sequence>
				     <xs:element name="series" type="xs:integer"/>
					 <xs:element name="number" type="xs:integer"/>
					 <xs:element name="surname" type="xs:string"/>
                     <xs:element name="name" type="xs:string"/>
                     <xs:element name="patronymic" type="xs:string"/>
                     <xs:element name="gender" type="xs:string"/>
                     <xs:element name="birthday" type="xs:date"/>
					 <xs:element name="locality" type="xs:string"/>
					 <xs:element name="city_name" type="xs:string"/>
					 <xs:element name="issued_by" type="xs:string"/>
					 <xs:element name="day_of_issue" type="xs:gDay"/>
					 <xs:element name="month_of_issue" type="xs:gMonth"/>
					 <xs:element name="year_of_issue" type="xs:gYear"/>
					 <xs:element name="current_date" type="xs:dateTime"/>
					 <xs:element name="start" type="xs:time"/>
				</xs:sequence>
			 </xs:complexType>
   </xs:element>
</xs:schema>