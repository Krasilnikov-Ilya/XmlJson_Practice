<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<xs:element name="passport">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="fildes" type="passportFildes" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="passportFildes">
		<xs:sequence>
			<xs:element name = "document_type" type = "xs:string"/>
			<xs:element name = "series" type = "xs:integer"/>
			<xs:element name = "number" type = "xs:integer"/>
			<xs:element name = "issued_by" type = "xs:string"/>
			<xs:element name = "issue_date" type = "xs:date"/>
			<xs:element name = "division_code" type = "xs:string"/>
			<xs:element name = "surname" type = "xs:string"/>
			<xs:element name = "first_name" type = "xs:string"/>
			<xs:element name = "patronymic" type = "xs:string"/>
			<xs:element name = "birth_date" type = "xs:date"/>
			<xs:element name = "gender" type = "xs:string"/>
			<xs:element name = "birth_place" type = "xs:string"/>
			<xs:element name = "registration" type = "xs:string"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>