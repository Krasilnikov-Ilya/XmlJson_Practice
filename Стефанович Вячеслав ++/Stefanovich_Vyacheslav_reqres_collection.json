		{
			"name": "Update + verify date",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"pm.test(\"Verify that job equals zion resident\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.job).to.eql(\"zion resident\");\r",
							"});\r",
							"pm.test(\"Verify that name equals zion morpheus\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.name).to.eql(\"morpheus\");\r",
							"});\r",
							"pm.test(\"verify date today \", function () {\r",
							" var jsonData = pm.response.json();\r",
							" var updatedAt = new Date(Date.parse(jsonData.updatedAt));\r",
							" updatedAt.setHours(0, 0, 0, 0);\r",
							" \r",
							" var now = new Date();\r",
							" now.setHours(0, 0, 0, 0);\r",
							"\r",
							" pm.expect(updatedAt).to.eql(now);  \r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
