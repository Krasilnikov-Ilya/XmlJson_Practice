			"name": "UPDATE проверка даты обновления без времени",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Проверка George Edwards имеет id 11 \", function () {\r",
							" var jsonData = pm.response.json();\r",
							" var updatedAt = new Date(Date.parse(jsonData.updatedAt));\r",
							" updatedAt.setHours(0, 0, 0, 0);\r",
							" \r",
							" var now = new Date();\r",
							" now.setHours(0, 0, 0, 0);\r",
							"\r",
							" pm.expect(updatedAt).to.eql(now);  \r",
							"});"
